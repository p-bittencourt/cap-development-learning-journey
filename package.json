{
  "name": "cap-development-learning-journey",
  "version": "1.0.0",
  "description": "A simple CAP project",
  "dependencies": {
    "@cap-js/sqlite": "^2.0.2",
    "@sap/cds": "^9",
    "express": "^4"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "db": {
        "[development]": {
          "kind": "sqlite",
          "credentials": {
            "url": ":memory:"
          }
        },
        "[testing]": {
          "kind": "sqlite",
          "credentials": {
            "url": "db.sqlite"
          }
        }
      }
    }
  }
}
